<%= link_to 'Назад', category_competency_skill_path(@category, @competency), class:'btn btn-primary' %>
<div class="container">
  <%= form_for([@category, @competency, @skill]) do |f| %>
    <div class="row mb-2">
      <div class="col-10 form-group">
        <%= f.label :title, "Название скилла" %>
        <%= f.text_field :title, class: 'form-control' %>
      </div>
      <%= f.submit 'Сохранить изменения', class: 'btn btn-success' %>
    </div>
  <% end %>
</div>